#!/bin/bash
CWD=$(cd $(dirname $0)/..; pwd)
cd $CWD

if [[ ! -f ./etc/agentd.rc ]]; then
	echo "file ${CWD}/etc/agentd.rc not found"
	exit 1
fi

if [[ ! -f ./bin/n9e-agentd ]]; then
	echo "file ${CWD}/bin/n9e-agentd not found"
	exit 1
fi

source ./etc/agentd.rc
./bin/n9e-agentd -f ./etc/agentd.yaml $*
